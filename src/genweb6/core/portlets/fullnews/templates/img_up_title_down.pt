<div class="portlet portlet-fullnews portlet-collection portlet-collection-iutd"
     tal:define="result_dicts view/result_dicts"
     tal:condition="result_dicts"
     i18n:domain="plone">

    <tal:block metal:use-macro="context/@@macros-genweb-portlet-fullnews/title"></tal:block>

    <ul class="ps-0 mb-2 row">

        <tal:item tal:repeat="obj result_dicts">

            <tal:block metal:use-macro="context/@@macros-genweb-portlets/img_up_title_down"></tal:block>

        </tal:item>

    </ul>

    <tal:block metal:use-macro="context/@@macros-genweb-portlet-fullnews/more_info"></tal:block>

</div>
